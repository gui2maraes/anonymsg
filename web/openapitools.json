{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.8.0",
    "generators": {
      "v3.0": {
        "inputSpec": "#{cwd}/openapi.yaml",
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/oapi",
        "additionalProperties": {
          "supportsES6": "true"
        }

      }
    }
  }
}
